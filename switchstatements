  break;
  }


switch (randomActivity) {
  case 0:
  activity = 'wrote a program';
  break;
  case 1:
  activity = 'debugged a program';
  break;
  case 2:
  eightBall = 'tested and evaluated a new program';
  break;
  case 3:
  eightBall = 'identified areas to modify a program';
  break;
  case 4:
  eightBall = 'got lost down a rabbit hole trying to create a new program';
  break;
}


